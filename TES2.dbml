//Todas las tablas tiene que tener id con el pk: valor unico
Table users {
    id serial [pk]
    username varchar [unique]
    email varchar
}

Table posts {
    id serial [pk]
    user_id int
    title varchar
    body text
    created_at timestamp
}

Table perfil{
    id serial [pk]
    info_personal varchar
    photos  


}
